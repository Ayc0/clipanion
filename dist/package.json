{
  "name": "@manaflair/concierge",
  "version": "0.9.4",
  "license": "MIT",
  "scripts": {
    "genparse": "pegjs -o sources/core/parse.js sources/core/command.peg",
    "build": "yarn genparse && babel -d dist/ sources/ && rm -f $(find dist -name '*.test.js')",
    "test": "mocha --compilers js:babel-register -r core-js $(find sources -name '*.test.js')",
    "push": "yarn build && cp README.md dist/ && jq 'del(.private)' package.json > dist/package.json && (cd dist && yarn publish)"
  },
  "dependencies": {
    "chalk": "^1.1.3",
    "kexec": "^3.0.0",
    "lodash": "^4.17.4"
  },
  "peerDependencies": {
    "joi": "10.x.x"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.3.2",
    "chai": "^3.5.0",
    "mocha": "^3.2.0",
    "pegjs": "^0.10.0"
  }
}
